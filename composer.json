{
  "name": "hostville/dorcas-laravel-compat",
  "homepage": "https://dorcas.com",
  "description": "Built to make Dorcas work seamlessly with Laravel",
  "keywords": ["dorcas","business","sdk","invoicing", "laravel"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Emmanuel Okeke",
      "homepage": "https://github.com/emmanix2002"
    }
  ],
  "require": {
